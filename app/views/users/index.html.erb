<div class="col-md-3"></div>
<div class="col-md-6">
  <% @users.each do |user| %>
    <% if user.id != current_user.id %>
      <div class="card mb-3">
        <div class="card-body">
          <%= image_tag user.image.url, class: 'user-image-picture' %>
          <div class="user-nickname-picture"><%= user.nickname %></div>
        </div>
      </div>
    <% end %>
  <% end %>
  <div class="col-md-3"></div>
<div class="row col-md-12"></div>
